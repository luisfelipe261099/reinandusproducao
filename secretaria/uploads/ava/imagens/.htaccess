# Permitir acesso a imagens
<FilesMatch "\.(jpg|jpeg|png|gif)$">
    Order Allow,Deny
    Allow from all
</FilesMatch>

# Negar acesso a outros tipos de arquivos
<FilesMatch "^(?!\.(jpg|jpeg|png|gif)$)">
    Order Deny,Allow
    Deny from all
</FilesMatch>

# Desativar listagem de diret√≥rios
Options -Indexes
